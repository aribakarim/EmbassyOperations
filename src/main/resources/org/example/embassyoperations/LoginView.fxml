<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.embassyoperations.LoginViewController">
   <children>
      <Label layoutX="229.0" layoutY="24.0" text="Login" wrapText="true">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label layoutX="100.0" layoutY="113.0" text="User Id">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="100.0" layoutY="183.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="userIdTextField" layoutX="101.0" layoutY="148.0" prefHeight="26.0" prefWidth="331.0" />
      <PasswordField fx:id="passwordPasswordField" layoutX="101.0" layoutY="221.0" prefHeight="26.0" prefWidth="331.0" />
      <Button layoutX="101.0" layoutY="261.0" mnemonicParsing="false" onAction="#loginButtonOnAction" prefHeight="27.0" prefWidth="331.0" text="Login" wrapText="true">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label layoutX="100.0" layoutY="301.0" text="Don't have an account?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button depthTest="ENABLE" layoutX="252.0" layoutY="301.0" mnemonicParsing="false" onAction="#signUpButtonOnAction" style="-fx-background-color: transparent; -fx-padding: 0;" text="Sign Up" textAlignment="CENTER" textFill="#1b5fcf" underline="true">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
