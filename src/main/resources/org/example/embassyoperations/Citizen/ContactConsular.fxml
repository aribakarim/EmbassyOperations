<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="450.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.embassyoperations.Citizen.ContactConsularController">
   <children>
      <VBox layoutX="119.0" layoutY="20.0" prefHeight="394.0" prefWidth="372.0">
         <children>
            <TextField fx:id="nameField" promptText="Enter Your Full Name">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="emailField" promptText="Enter Your Email">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="subjectField" promptText="Subject">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
            <TextArea fx:id="messageArea" prefHeight="200.0" prefWidth="200.0" promptText="Type your message here...">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextArea>
            <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitOnAction" text="Send Message">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
